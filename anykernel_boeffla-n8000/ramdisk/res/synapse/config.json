            {
               STitleBar:{
                  title:"CPUFreq scaling"
               }
            },
            {
               SSeekBar:{
                  title:"CPU max frequency",
                  description:"Set the maximum freqency the CPU scales up to.",
                  unit:"MHz",
                  step:100000,
                  weight:0.001,
                  min:200000,
                  max:1704000,
                  default:1600000,
                  action:"generic /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq"
