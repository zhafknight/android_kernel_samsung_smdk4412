               SPane:{
                  title:"I/O schedulers",
                  description:"Set the active I/O elevator algorithm. The scheduler decides how to handle I/O requests and how to handle them."
               }
            },
            {
               SOptionList:{
                  title:"Internal storage scheduler",
                  default:cfq,
